{
  "name": "MyErp Code Generation",
  "projections": {
    "persistence": {
      "packageId": "ShapeFlow.Projections.Persistence",
      "version": "1.0.0",
      "input": {
        "format": "json"
      },
      "output": {
        "format": "fileset",
        "loaderName": "fileSetLoader"
      }
    }
  },
  "shapes": {
    "salesModel" :  {
      "tags": "domainEntityModel",
      "loaderName": "JsonLoader",
      "parameters": {
        "model-path": "Shapes\\Order.json"
      }
    }
  },
  "pipelines": {
    "applicationTier" : {
      "stages": {
        "entities": {
          "selector": "salesModel",
          "projectionRef": "persistence"
        }
      }
    }
  }
}