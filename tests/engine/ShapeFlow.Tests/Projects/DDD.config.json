{
  "name": "My Application 1",
  "projections": {
    "DDDGenerator": {
      "version": "1.0.0",
      "rulesBasePath": "Rules",
      "rules": [
        {
          "fileName": "DomainObjects\\Aggregates.liquid"
        }
      ],
      "input": {
        "format": "json"
      },
      "output": {
        "format": "fileset",
        "loaderName": "fileSetLoader"
      }
    }
  },
  "shapes": {
    "SalesModel": {
      "tags": "domainEntityModel",
      "loaderName": "JsonLoader",
      "parameters": {
        "model-path": "Shapes\\Order.json"
      }
    }
  },
  "pipelines": {
    "applicationTier": {
      "stages": {
        "domainEntities": {
          "selector": "SalesModel",
          "projectionRef": "DDDGenerator"
        }
      }
    }
  }
}